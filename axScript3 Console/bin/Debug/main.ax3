//Some testing stuff

#module "Ax/STD.dll"
#module "Ax/IO.dll"

// Reads all lines into an array.
~(readAllLines: fileName |
	(set ^handle (fileopen fileName "r"))
	(set ^tmp (array))
	(do {
		(set ^line (handle.readline))
		(insert tmp line)
	} %{(neq line null)})
	(handle.close)
	
	(return tmp)
)

~(main: args |
	(
		~[EntryPoint]

		(set ^lines (readAllLines (read "Please enter a file to read")))
		
		(printarr lines)
	)
)

